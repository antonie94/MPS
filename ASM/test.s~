.INCLUDE "M32DEF.INC"
.ORG 0
		LDI R16,HIGH(RAMEND)
		OUT SPH,R16
		LDI R16,LOW(RAMEND)
		OUT SPL,R16
		SER R16
		OUT DDRD,R16
		LDI R16,7

MAIN:	SBI PORTD,0
		RCALL DELAY_1S
		SBI PORTD,1
		RCALL DELAY_1S
		SBI PORTD,2
		RCALL DELAY_1S
		SBI PORTD,3
		RCALL DELAY_1S
		SBI PORTD,4
		RCALL DELAY_1S
		SBI PORTD,5
		RCALL DELAY_1S
		SBI PORTD,6
		RCALL DELAY_1S
		SBI PORTD,7
		RCALL DELAY_1S
		CLR R16
		OUT PORTD,R16
		RJMP MAIN





;===DELAY_1S

DELAY_1S:
		LDI R20,55
DELAY1:	LDI R21,200
DELAY2:	LDI R22,250
DELAY3:	NOP
		DEC R22
		BRNE DELAY3
		DEC	R21
		BRNE DELAY2
		DEC R20
		BRNE DELAY1
		RET
